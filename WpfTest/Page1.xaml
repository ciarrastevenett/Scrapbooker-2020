<Page x:Class="WpfTest.Page1"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfTest"
      mc:Ignorable="d" 
      Title="Page1" Height="450" Width="800" Background="White">

    <Grid>
        <Frame x:Name= "SelectionPage"/>
        <RadioButton x:Name="Scanned_Button" Content="Scan" IsChecked ="True" HorizontalAlignment="Left" Margin="247,220,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name="Downloaded_Button" Content="Download" HorizontalAlignment="Left" Margin="483,223,0,0" VerticalAlignment="Top"/>
        <Button Content="Continue" HorizontalAlignment="Center" Margin="150,290,0,0" VerticalAlignment="Top" Width="74" Click="Button_Click"/>
        <ComboBox HorizontalAlignment="Left" Margin="280,290,0,0" VerticalAlignment="Top" Width="120">
            <TextBlock> Create an album</TextBlock>
            <TextBlock> Locations</TextBlock>
            <TextBlock> Favorites</TextBlock>
            <TextBlock> Childhood memories</TextBlock>

        </ComboBox>


    </Grid>
</Page>
